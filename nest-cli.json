{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/talk-addictive/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/talk-addictive/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/talk-addictive",
  "projects": {
    "talk-addictive": {
      "type": "application",
      "root": "apps/talk-addictive",
      "entryFile": "main",
      "sourceRoot": "apps/talk-addictive/src",
      "compilerOptions": {
        "tsConfigPath": "apps/talk-addictive/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "feature-auth": {
      "type": "library",
      "root": "libs/feature-auth",
      "entryFile": "index",
      "sourceRoot": "libs/feature-auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/feature-auth/tsconfig.lib.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "main",
      "sourceRoot": "apps/chat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/tsconfig.app.json"
      }
    },
    "feature-chat": {
      "type": "library",
      "root": "libs/feature-chat",
      "entryFile": "index",
      "sourceRoot": "libs/feature-chat/src",
      "compilerOptions": {
        "tsConfigPath": "libs/feature-chat/tsconfig.lib.json"
      }
    }
  }
}