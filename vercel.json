{
  "version": 2,
  "scope": "talk_addictive_version_scope",
  "github": {
    "enabled": true
  },
  "builds": [
    {
      "src": "/dist/apps/auth/main.ts",
      "use": "@now/node"
    },
    {
      "src": "/dist/apps/chat/main.ts*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": ["/dist/apps/auth/main.js", "dist/apps/chat/main.js"],
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    }
  ]
}
