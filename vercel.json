{
  "version": 2,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "apps/auth/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/chat/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/auth/src/main.ts",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "apps/chat/src/main.ts", 
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE"]
    }
  ]
}
